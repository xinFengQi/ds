{
    "manifest_version": 2,
    "name": "ds_extension",
    "version": "1.0.0",
    "description": "ds_extension插件",
    "icons": {
        "16": "img/icon.png",
        "48": "img/icon.png",
        "128": "img/icon.png"
    },
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_title": "ds_extension插件",
        "default_popup": "html/popup.html"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["js/auto_login.js"],
        "css": [],
        "run_at": "document_start"
    }],
    "permissions": [
        "contextMenus",
        "tabs",
        "notifications",
        "webRequest",
        "webRequestBlocking",
        "storage",
        "http://*/*",
        "https://*/*"
    ]
}