{
    "manifest_version": 2,
    "name": "__MSG_extensionName__",
    "version": "1.0.0",
    "description": "__MSG_extensionName__",
    "icons": {
        "16": "img/icon.png",
        "48": "img/icon.png",
        "128": "img/icon.png"
    },
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_title": "__MSG_extensionName__",
        "default_popup": "html/popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["js/util/inject.js", "js/auto_login.js", "js/index.js"],
            "css": [],
            "run_at": "document_start"
        }
    ],
    "background":
	{
		"page": "html/background.html"
	},
    "web_accessible_resources": ["js/global.js"],
    "homepage_url": "https://www.cnblogs.com/liuxianan/p/chrome-plugin-develop.html",
    "chrome_url_overrides":
	{
		"newtab": "html/home.html"
    },
    "options_ui":
	{
		"page": "html/setting.html"
    },
    "devtools_page": "html/devtools.html",
    "default_locale": "zh_CN",
    "permissions": [
        "background",
        "contextMenus",
        "tabs",
        "notifications",
        "webRequest",
        "webRequestBlocking",
        "storage",
        "bookmarks",
        "http://*/*",
        "https://*/*"
    ]
}