{
  "name": "bootstrap5_stencil",
  "version": "0.0.1",
  "description": "使用bootstrap4构建自己的标准组件",
  "main": "./dist/dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "./dist/dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/ds/ds.esm.js",
  "files": [
    "./dist/dist/",
    "./dist/dist/loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start:stencil": "stencil build --docs --dev --watch --serve",
    "dev": "node ./scripts/monitor.js",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "g": "stencil generate"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "@stencil/core": "^2.0.1",
    "bootstrap": "^5.0.2",
    "bootstrap-icons": "^1.5.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@stencil/store": "^1.4.1",
    "@types/node": "^15.12.4",
    "browser-sync": "^2.27.9",
    "npm-run-all": "^4.1.5"
  },
  "author": ""
}
