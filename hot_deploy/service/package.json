{
  "name": "hot_deploy_service",
  "version": "1.0.0",
  "description": "热部署服务",
  "main": "index.js",
  "scripts": {
    "dev": "set NODE_ENV=development && nodemon",
    "build": "tsc",
    "pm2": "pm2 start ./dist/index.js --name 'hotdeploy_server'  --env  NODE_ENV='production' --output ./pm2logs/logs-out.log  --error ./pm2logs/logs-error.log  --watch dist",
    "pm2_reload": "pm2 reload 'hotdeploy_server' ",
    "start": "ts-node src/index.ts"
  },
  "author": "damon.dong",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.7",
    "@types/fs-extra": "^9.0.1",
    "@types/node": "^14.0.24",
    "@types/node-schedule": "^1.3.0",
    "@types/os-utils": "^0.0.0",
    "@types/request": "^2.48.5",
    "@types/socket.io": "^2.1.11",
    "@types/socket.io-client": "^1.4.33",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "iconv-lite": "^0.6.2",
    "moment": "^2.27.0",
    "node-schedule": "^1.3.2",
    "nodemon": "^2.0.4",
    "os-utils": "^0.0.14",
    "request": "^2.88.2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
