{
	"name": "vscode-extensio-test1",
	"displayName": "vscode-extensio-test1",
	"description": "测试的vscode插件",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"menus": {
            "editor/context": [
                {
                    "when": "editorFocus",
                    "command": "vscode-extensio-test1.getCurrentFilePath",
                    "group": "navigation"
                }
            ],
            "explorer/context": [
                {
                    "command": "vscode-extensio-test1.getCurrentFilePath",
                    "group": "navigation"
                }
            ]
        },
		"commands": [
			{
				"command": "vscode-extensio-test1.helloWorld",
				"title": "ds/vscode插件.hellowWorld"
			},
			{
				"command": "vscode-extensio-test1.showWelcome",
				"title": "ds/vscode插件.主页"
			},
			{
				"command": "vscode-extensio-test1.getCurrentFilePath",
				"title": "ds/vscode插件.getCurrentFilePath"
			}
		],
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets/javascript.json"
			}
		],
		"configuration": {
			"type": "object",
			"title": "ds/vscode插件测试",
			"properties": {
				"vscodePluginDemo.yourName": {
					"type": "string",
					"default": "guest",
					"description": "你的名字"
				},
				"vscodePluginDemo.showTip": {
					"type": "boolean",
					"default": true,
					"description": "启动时显示自定义欢迎页"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.63.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4",
		"ts-loader": "^9.2.6",
		"webpack": "^5.66.0",
		"webpack-cli": "^4.9.1",
		"@vscode/test-electron": "^2.0.3"
	}
}